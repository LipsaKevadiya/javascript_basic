{
  "env": {
    "browser": true,
    "es2021": true
  },

  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": ["error"],
    "no-console": ["warn"],
    "consistent-return": ["error"],
    "no-unused-vars": ["error"],
    "indent": ["error", 2],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"]
  }
}
